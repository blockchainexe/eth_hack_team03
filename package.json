{
  "name": "kayac-html5-starter",
  "version": "0.0.0",
  "description": "kayac-html5-starter development.",
  "author": "Kayac Inc.",
  "license": "MIT",
  "scripts": {
    "clean": "truffle networks --clean && rm -rf ./build/contracts/*",
    "build": "webpack --mode production --progress",
    "start": "webpack-dev-server --mode development --inline --open",
    "deploy": "truffle deploy --network testnet",
    "predeploy:site": "npm run build",
    "deploy:site": "gh-pages -d public"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "extract-text-webpack-plugin": "next",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "js-yaml-loader": "^0.1.0",
    "material-design-lite": "^1.3.0",
    "node-sass": "^4.8.3",
    "portfinder": "^1.0.13",
    "postcss-loader": "^2.1.3",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "read-config": "^2.0.0",
    "reset.css": "^2.0.2",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "webpack": "^4.16.1",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.4",
    "webpack-route-data-mapper": "^2.0.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "browserslist": [
    "ie >= 11",
    "ios >= 9",
    "android >= 4.4"
  ],
  "dependencies": {
    "add": "^2.0.6",
    "aws-sdk": "^2.279.1",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.2",
    "dirty-chai": "^2.0.1",
    "jquery": "^3.3.1",
    "uport-connect": "^0.7.3"
  }
}
